{
    "version": 2,
    "builds": [
        {
            "src": "machaheadq/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "machaheadq/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "machaheadq.settings",
        "PYTHONUNBUFFERED": "1"
    }
}
