{
    "version": 2,
    "builds": [
        {
            "src": "machaheadq/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/tweet/.*",
            "dest": "machaheadq/wsgi.py"
        },
        {
            "src": "/.*",
            "dest": "machaheadq/wsgi.py"
        }
    ]
}
